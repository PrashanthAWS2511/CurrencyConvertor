{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error"
    }
  },
  "AllowedHosts": "*",
  "ExchangeRatesApi": {
    "BaseUrl": "https://api.frankfurter.dev/v1",
    "Timeout": 30
  },
  "Jwt": {
    "Key": "ProdSuperSecretKey",
    "Issuer": "ProdIssuer",
    "Audience": "ProdAudience"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://prod-seq-server:5341" }
      }
    ]
  }
}