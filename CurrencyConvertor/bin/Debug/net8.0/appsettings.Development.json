{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ExchangeRatesApi": {
    "BaseUrl": "https://api.frankfurter.dev/v1",
    "Timeout": 30
  },
  "Jwt": {
    "Key": "YourSuperSecretKeyHere1234567890",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  }
}